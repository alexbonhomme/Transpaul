{
  "manifest_version": 2,

  "name": "Transpaul",
  "description": "This extension help me to catch my bus!",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://www.kimonolabs.com/"
  ],
  "content_scripts": [ {
    "js": [ "jquery.min.js", "moment.min.js", "transpaul.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
